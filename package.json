{
  "name": "game-voice",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "deploy": "npm run build && node dist/scripts/deploy-commands.js",
    "deploy:dev": "node --loader ts-node/esm src/scripts/deploy-commands.ts",
    "deploy:prod": "ts-node src/scripts/deploy-commands.ts",
    "clean-deploy": "ts-node src/scripts/deploy-commands.ts --clean",
    "clean": "node --loader ts-node/esm src/scripts/clean-commands.ts"
  },
  "dependencies": {
    "@discordjs/builders": "^1.11.0",
    "@discordjs/rest": "^2.4.3",
    "@discordjs/voice": "^0.18.0",
    "@discordjs/ws": "^2.0.1",
    "@sapphire/snowflake": "^3.5.5",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^9.14.0",
    "axios": "^1.9.0",
    "bottleneck": "^2.19.5",
    "colors": "^1.4.0",
    "discord-api-types": "^0.38.1",
    "discord.js": "^14.18.0",
    "dotenv": "^16.5.0",
    "ExtendedClient": "file:./structs/ExtendedClient",
    "fast-glob": "^3.3.3",
    "limiter": "^3.0.0",
    "lowdb": "^7.0.1",
    "lru-cache": "^11.1.0",
    "module-alias": "^2.2.3",
    "node-cache": "^5.1.2",
    "python3": "^0.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.2",
    "@types/node-cache": "^4.1.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "glob": "^11.0.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.2.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "_moduleAliases": {
    "/utils": "dist/utils"
  }
}
